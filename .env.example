# n8n API Configuration
N8N_API_URL=http://localhost:5678/api/v1
N8N_API_KEY=your-api-key

# Framework Configuration
N8N_TEMPLATES_DIR=./templates
N8N_TESTS_DIR=./tests

# Credential Environment Variables
# Format: N8N_CREDENTIAL_<NAME>_<PROPERTY>

# HTTP Basic Auth Credential Example
N8N_CREDENTIAL_API_TYPE=httpBasicAuth
N8N_CREDENTIAL_API_USERNAME=your-username
N8N_CREDENTIAL_API_PASSWORD=your-password

# OAuth2 Credential Example
N8N_CREDENTIAL_OAUTH_TYPE=oAuth2Api
N8N_CREDENTIAL_OAUTH_CLIENT_ID=your-client-id
N8N_CREDENTIAL_OAUTH_CLIENT_SECRET=your-client-secret
N8N_CREDENTIAL_OAUTH_ACCESS_TOKEN_URL=https://example.com/oauth/token

# Database Credential Example
N8N_CREDENTIAL_DB_TYPE=mySql
N8N_CREDENTIAL_DB_HOST=localhost
N8N_CREDENTIAL_DB_PORT=3306
N8N_CREDENTIAL_DB_DATABASE=your-database
N8N_CREDENTIAL_DB_USER=your-db-user
N8N_CREDENTIAL_DB_PASSWORD=your-db-password

# Docker Configuration
N8N_CONTAINER_NAME=n8n
N8N_IMAGE=n8nio/n8n
N8N_PORT=5678
