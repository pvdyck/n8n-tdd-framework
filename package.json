{
  "name": "n8n-tdd-framework",
  "version": "0.13.0",
  "description": "A Test-Driven Development framework for n8n workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "n8n-tdd": "./bin/n8n-tdd.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:coverage:check": "node dist/testing/coverage/utils/thresholdChecker.js",
    "test:coverage:dashboard": "node dist/testing/coverage/dashboard/dashboardGenerator.js",
    "test:coverage:dashboard:static": "open public/static-dashboard.html",
    "test:coverage:clean": "node dist/testing/coverage/utils/fileManager.js clean",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prepublishOnly": "npm test",
    "docs": "typedoc --out docs/api src/index.ts",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "testing",
    "tdd",
    "coverage"
  ],
  "author": "pvdyck",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/pvdyck/n8n-tdd-framework"
  },
  "bugs": {
    "url": "https://github.com/pvdyck/n8n-tdd-framework/issues"
  },
  "homepage": "https://github.com/pvdyck/n8n-tdd-framework#readme",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "chart.js": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typedoc": "^0.25.12"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
